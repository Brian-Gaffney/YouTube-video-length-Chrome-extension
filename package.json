{
  "name": "youtube_video_length",
  "version": "2.0.0",
  "description": "YouTube video length Chrome extension",
  "scripts": {
    "dev": "rm -rf build && NODE_ENV=development webpack --config webpack/dev.babel.js --progress --colors --watch",
    "build": "rm -rf build && NODE_ENV=production webpack --config webpack/prod.babel.js --progress --colors -p --display-chunks --display-modules",
    "lint": "./node_modules/.bin/eslint --ext .js ./src ./webpack",
    "lint-fix": "./node_modules/.bin/eslint --fix --ext .js ./src ./webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brian-Gaffney/YouTube-video-length-Chrome-extension.git"
  },
  "author": "Brian Gaffney",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Brian-Gaffney/YouTube-video-length-Chrome-extension/issues"
  },
  "homepage": "https://github.com/Brian-Gaffney/YouTube-video-length-Chrome-extension#readme",
  "dependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bluebird": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-plugin-import": "^2.2.0",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^4.5.2",
    "query-string": "^4.3.4",
    "resolve-url-loader": "^2.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "tether-tooltip": "^1.2.0",
    "webpack": "^2.4.1",
    "yarn": "^0.23.3",
    "zip-webpack-plugin": "^1.1.0"
  }
}
